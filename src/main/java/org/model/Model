
public class Model {
    
    human (String name, int age, String gender, LocalDate birthdate, health) {
        this.name = name;
        this.age = age;
        this.gender = gender;
        this.birthdate = birthdate; 
    }

    activity (String name, String location,  LocalDate date) {
        this.activity = activity;
        this.location = location;
        this.date = date;
    }

    //healthstate can be affected by activities

    health (healthValues, int weight, int heartRate, Int bloodPressure,  String mood) {
        this.name = name;
        this.weight = weight;
        this.height = height;
        this.bloodPressure = bloodPressure;
        this.heartRate = heartRate;
    }

    healthValues(100 - 80 = good, 79 - 60 = okay, 59 - 0 = bad) {
        this.healthValues = healthValues;
    }

    
}

